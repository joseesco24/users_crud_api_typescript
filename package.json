{
	"name": "users_crud_api_typescript",
	"version": "0.0.0",
	"description": "A really simple CRUD GraphQL API based on Docker and TypeScript.",
	"author": {
		"email": "josed.escobar@outlook.com",
		"name": "joseesco24"
	},
	"readme": "readme.md",
	"_moduleAliases": {
		"@artifacts": "./dist/artifacts",
		"@database": "./dist/database",
		"@dtos": "./dist/dtos",
		"@entities": "./dist/entities",
		"@graphql_resolvers": "./dist/graphql_resolvers",
		"@graphql_routers": "./dist/graphql_routers",
		"@graphql_services": "./dist/graphql_services",
		"@interceptors": "./dist/interceptors",
		"@middlewares": "./dist/middlewares",
		"@rest_controllers": "./dist/rest_controllers",
		"@rest_routers": "./dist/rest_routers",
		"@rest_services": "./dist/rest_services"
	},
	"scripts": {
		"format": "prettier --config ./.prettierrc.json --write \"src/**/*.ts\"",
		"lint": "eslint --config ./.eslintrc.json \"src/**/*.ts\" --fix",
		"postinstall": "npx husky install",
		"start-dev": "nest start --watch",
		"start-app": "node dist/main",
		"prebuild": "rimraf dist",
		"build": "nest build"
	},
	"dependencies": {
		"@nestjs/axios": "^2.0.0",
		"@nestjs/cli": "^9.3.0",
		"@nestjs/common": "^9.3.12",
		"@nestjs/config": "^2.3.1",
		"@nestjs/core": "^9.3.12",
		"@nestjs/platform-express": "^9.3.12",
		"@nestjs/typeorm": "^9.0.1",
		"axios": "^1.3.4",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.0",
		"helmet": "^6.0.1",
		"joi": "^17.9.1",
		"module-alias": "^2.2.2",
		"npm": "^9.6.2",
		"pg": "^8.10.0",
		"pino": "^8.11.0",
		"pino-http": "^8.3.3",
		"pino-pretty": "^10.0.0",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^4.4.1",
		"rxjs": "^7.8.0",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.5.0",
		"@commitlint/config-conventional": "^17.4.4",
		"@nestjs/schematics": "^9.0.4",
		"@nestjs/testing": "^9.3.12",
		"@types/express": "^4.17.17",
		"@types/jest": "29.5.0",
		"@types/module-alias": "^2.0.1",
		"@types/node": "^18.15.9",
		"@types/pg": "^8.6.6",
		"@types/supertest": "^2.0.12",
		"@types/uuid": "^9.0.1",
		"@typescript-eslint/eslint-plugin": "^5.56.0",
		"@typescript-eslint/parser": "^5.56.0",
		"eslint": "^8.36.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.3",
		"jest": "^29.5.0",
		"prettier": "^2.8.7",
		"source-map-support": "^0.5.21",
		"supertest": "^6.3.3",
		"ts-jest": "^29.0.5",
		"ts-loader": "^9.4.2",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.1.2",
		"typescript": "^5.0.2"
	},
	"jest": {
		"coverageDirectory": "../coverage",
		"testRegex": ".*\\.spec\\.ts$",
		"testEnvironment": "node",
		"rootDir": "src",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"moduleFileExtensions": [
			"json",
			"js",
			"ts"
		],
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		]
	}
}
